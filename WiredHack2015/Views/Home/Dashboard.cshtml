@section styles{
    @Styles.Render("~/Content/DashboardCss")
}
@section scripts{
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAHHCY6JtRuBrmAh_KDOA4npMi1GgR4rGo">

    </script>
    <script type="text/javascript">
        var map;
        function initialize() {
            var mapOptions = {
                zoom: 8,
                center: new google.maps.LatLng(-34.397, 150.644)
            };
            map = new google.maps.Map(document.getElementById('map-canvas'),
                mapOptions);
        }

        google.maps.event.addDomListener(window, 'load', initialize);
    </script>
}
<div class="row">
    <div class="col-sm-2 col-md-2 sidebar">
        <ul class="nav nav-sidebar">
            <li>
                <form class="navbar-form">

                    <input type="text" class="form-control" placeholder="Search Zip"/>
                    <br/>
                    <button type="submit" class="btn btn-default" style="margin-top: 10px;">Search</button>
                </form>
            </li>
        </ul>
    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main" id="map-canvas">
        

    </div>
</div>
